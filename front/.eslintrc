{
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module", // module system : import & export
    "ecmaFeatures": {
      // 특별히 쓰는 기능
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["import", "react-hooks"],
  "rules": {}
}
